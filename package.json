{
  "name": "from-node-stream",
  "version": "0.0.1",
  "description": "convert nodejs-stream into webstream",
  "keywords": [],
  "homepage": "https://github.com/snomiao/from-node-stream#readme",
  "bugs": {
    "url": "https://github.com/snomiao/from-node-stream/issues"
